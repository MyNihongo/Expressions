<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net40;net5.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<Authors>MyNihongo</Authors>
		<Product>MyNihongo</Product>
		<Description>Utility methods for System.Linq.Expression.Expression</Description>
		<Copyright>Copyright Â© 2022 MyNihongo</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>0.2.0</Version>
		<GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">true</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/MyNihongo/Expressions</PackageProjectUrl>
		<RepositoryUrl>https://github.com/MyNihongo/Expressions</RepositoryUrl>
		<PackageIcon>favico.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\favico.png">
	    <Pack>True</Pack>
	    <PackagePath />
	  </None>
	</ItemGroup>

</Project>
